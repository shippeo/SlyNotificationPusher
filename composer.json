{
    "name": "sly/notification-pusher",
    "description": "Standalone PHP library for easy devices notifications push (Fork from https://github.com/Ph3nol/NotificationPusher)",
    "keywords": [
        "apple",
        "iphone",
        "apns",
        "android",
        "gcm",
        "notification",
        "message",
        "push",
        "pusher"
    ],
    "homepage": "https://github.com/shippeo/SlyNotificationPusher",
    "type": "standalone",
    "license": "MIT",
    "authors": [
        {
            "name": "CÃ©dric Dugat",
            "email": "cedric@dugat.me"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/shippeo/SlyNotificationPusher/contributors"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-ctype": "*",
        "symfony/options-resolver": ">=2.3",
        "symfony/console": ">=2.3",
        "zendframework/zendservice-apple-apns": "~1.4",
        "zendframework/zendservice-google-gcm": "~2.1",
        "zendframework/zend-validator": "^2.12",
        "doctrine/inflector": "^1.1",
        "symfony/filesystem": ">=2.3"
    },
    "require-dev": {
